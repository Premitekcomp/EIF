<div class="warper container-fluid">
    <div class="panel panel-default">
        <div class="panel-heading">
            Amounts Collected From Alex And Cairo Team
          
        </div>
        <div class="panel-body">
            @{
                if (ViewBag.error != null)
                {
                    <p class="text-danger"><span class="fa fa-times-circle text-red"></span>Error : @ViewBag.error</p>
                }}
            @Html.Partial("_FilterPayment")

            <div id="container">
                <ul class="nav nav-tabs">
                    <li class="active"><a data-toggle="tab" href="#activeDomain">Active</a></li>
                    <li><a data-toggle="tab" href="#woDomain">W/O</a></li>
                    <li><a data-toggle="tab" href="#allDomains">All</a></li>
                </ul>
                <br/>
                <div class="tab-content">
                    <div id="activeDomain" class="scrollmenu tab-pane fade in active">
                        
                        @Html.Partial("_ReportPayment")
                    </div>

                    <div id="woDomain" class="tab-pane fade">

                    </div>
                    <div id="allDomains" class="tab-pane fade">

                    </div>

                </div>
                </div>

        </div>
    </div>
</div>

<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<script src="~/Scripts/jquery-1.10.2.js"></script>
<script src="~/Scripts/bootstrap.min.js"></script>
<link href="~/Content/MultiSelect/bootstrap-multiselect.min.css" rel="stylesheet" />
<script src="~/Scripts/MultiSelect/bootstrap-multiselect.min.js"></script>
<script src="~/Scripts/Custom/ExportExcel.js"></script>







