
<div class="panel panel-default">
    <div class="panel-body">
        @{if (ViewBag.error!=null)
          {
                <p class="text-danger"><span class="fa fa-times-circle text-red"></span> Error: @ViewBag.error</p> 
          }
          if (ViewBag.RowsSuccess != null)
          {
            <p class="text-primary"><span class="fa fa-check-circle text-blue"></span> Number Of Success :  @ViewBag.RowsSuccess Row</p>
          }
            if (ViewBag.RowsFailure!=null)
            {
                <p class="text-danger"><span class="fa fa-times-circle text-red"></span> Number Of Failure : @ViewBag.RowsFailure</p>
            <div class="row form-group">
                <div class="col-lg-2"></div>
                <div class="col-lg-10">
                    @{
                if (ViewBag.FailureRowsSummary!=null)
                {
                    foreach (string S in ViewBag.FailureRowsSummary)
                {
                     <p class="text-danger"><span class="fa fa-times-circle text-red"></span> @S</p>
                }
                }
                
                    }
                </div>
            </div>
           
            
               
            }
          }
</div>
</div>
