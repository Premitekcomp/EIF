
<div class="warper container-fluid">

    <div class="panel panel-default">
        <div class="panel-heading">
            DCR Domains
        </div>
        <div class="panel-body">

            <div id="container">

                <table cellpadding="0" border="0" class="table table-striped table-bordered" id="tbl3">
                    <thead>
                        <tr>
                            <th>Domain Title</th>
                            <th>Branch</th>
                            <th>Bank</th>
                            <th>Options</th>
                        </tr>
                    </thead>
                    <tbody>
                        @{
                            if (ViewBag.Data != null)
                            {
                                IEnumerable<EIF.Controllers.collectionController.DCRDOmainData> data = ViewBag.Data;
                                foreach (var R in data)
                                {
                                    <tr>
                                        <td>@R.DomainName</td>
                                        <td>@R.BranchName</td>
                                        <td>@R.AgencyName</td>
                                        <td>
                                            <a target="_blank" href="@Url.Action("GetDCRDataDetails", "collection", new { DomainID = R.DomainID })" class="btn btn-default tooltip-btn"> View</a>   
                                        </td>
                                    </tr>
                                }
                            }}
                    </tbody>


                </table>

            </div>

        </div>
    </div>
</div>

<script src="~/Scripts/Custom/PublicConfig.js"></script>
<script src="~/Scripts/Custom/Collection.js"></script>
<link href="~/Content/assets/css/plugins/datatables/jquery.dataTables.css" rel="stylesheet" />
<script src="~/Scripts/jquery-1.10.2.js"></script>
<script src="~/Content/assets/js/plugins/datatables/jquery.dataTables.js"></script>
<script src="~/Content/assets/js/plugins/datatables/DT_bootstrap.js"></script>
<script src="~/Content/assets/js/plugins/datatables/jquery.dataTables-conf.js"></script>
<script>
    $('#tbl3').dataTable({
        "pageLength": 10,
        "paging": true
    });
</script>
<style>
    .col-sm-2 {
        width: 100px !important;
    }

    .col-sm-3 {
        width: 200px !important;
    }
</style>
