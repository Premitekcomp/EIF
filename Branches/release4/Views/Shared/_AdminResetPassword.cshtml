
@{
    string URL = EIF.PublicConfig.URL;
    string sideurl = EIF.PublicConfig.sideurl;

}
@{if (ViewBag.ID != null)
  {

<fieldset>
    <input type="hidden" id="id" name="id" value="@ViewBag.ID" />

    <div class="form-group input-group">
        <span class="input-group-addon"><i class="fa fa-key"></i></span>
        <input type="password" name="NewPass" placeholder="New Password" class="form-control">
    </div>
 

    <div class="form-group input-group">
        <span class="input-group-addon"><i class="fa fa-key"></i></span>
        <input type="password" name="ConfirmPass" placeholder="Confirm New Password" class="form-control">
    </div>
 
    <a>
        <input type="submit" value="Change" class="btn btn-purple btn-block" />
    </a>
    <hr>
 </fieldset>
}}