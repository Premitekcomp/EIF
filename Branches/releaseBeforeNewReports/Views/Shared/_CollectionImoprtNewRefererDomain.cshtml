@{EIF.Helper.collection.Get get = new EIF.Helper.collection.Get();}
@{EIF.Models.Dom_DomainList data = ViewBag.data;}
@using (Ajax.BeginForm("ImportDomain", "collection",
                                     new AjaxOptions
                                     {
                                         //AllowCache = false,
                                         HttpMethod = "POST",
                                         UpdateTargetId = "ImportMsg",
                                         InsertionMode = InsertionMode.Replace,
                                         OnBegin = "ShowProgress();"
                                         ,
                                         OnComplete = "HideProgress();"
                                         // OnSuccess = "Step3"

                                     }))
{
    <div class="panel panel-default">
        <div class="panel-heading">
            Import Domain
        </div>
        <div class="panel-body">
            <div id="container">
                <div class="col-lg-12">

                    <div class="row">
                        <div class="form-group">
                            <div class="row">
                                <div class="col-lg-4">
                                   
                                    <input type="hidden" id="DomainID2" name="DomainID2" value="@data.DomainID" />
                                    <input type="hidden" id="SchemaID" name="SchemaID" value="@data.SchemaID" />
                                    <input type="hidden" id="VersionNumber" name="VersionNumber" value="@data.VersionNumber" />
                                    <br />
                                   <label>Schema Name : </label> <span>@data.SchemaName</span>
                                </div>
                                <div class="col-lg-4">
                                    <br />
                                    <center>
                                        <span id="selectF" class="btn btn-success fileinput-button" style="width:200px">
                                            <i class="glyphicon glyphicon-plus"></i>
                                            <span>Select Domain File</span>
                                            <input type="file" name="files[]" id="file">
                                        </span>
                                    </center>
                                    <input type="hidden" name="excelFile" id="excelFile" value="" />
                                </div>
                                <div class="col-lg-4"></div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group">
                            <div class="row" id="sheets"> </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group">
                            <div class="row">
                                <div id="Statistic"></div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>


        </div>
    </div>

}

<script src="~/Scripts/Custom/PublicConfig.js"></script>
<script src="~/Scripts/Custom/Collection.js"></script>
