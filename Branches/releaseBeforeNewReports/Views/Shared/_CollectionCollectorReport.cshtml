
@{
    <div class="row form-group">
                                    <div class="col-lg-3">
                                        <label>Select Year </label>
                                        <input type="text" id="datepicker" />
                                        <label>  Select Month </label>
                                        <select id='gMonth2' onchange="show_month()">
                                            <option value=''>--Select Month--</option>
                                            <option selected value='1'>Janaury</option>
                                            <option value='2'>February</option>
                                            <option value='3'>March</option>
                                            <option value='4'>April</option>
                                            <option value='5'>May</option>
                                            <option value='6'>June</option>
                                            <option value='7'>July</option>
                                            <option value='8'>August</option>
                                            <option value='9'>September</option>
                                            <option value='10'>October</option>
                                            <option value='11'>November</option>
                                            <option value='12'>December</option>
                                        </select> 
                                        <button class="btn btn-default tooltip-btn" onclick="">Apply</button>
                                    </div>
                                    
        </div>
    
                         if (ViewBag.CollectorReport != null)
                            {
                                List<EIF.Helper.CollectorwithPayment> Res = ViewBag.CollectorReport;
                                            <table class="table no-margn">
                                                <thead>
                                                    <tr>
                                                        <th>Collector Code</th>
                                                        <th>Collector Name</th>
                                                        <th>Domain Type </th>
                                                        <th>Product</th>
                                                        <th>Count Of Customers</th>
                                                        <th>Count Of Payment</th>
                                                        <th>Total Payment</th>
                                                        <th>Past Due</th>
                                                        <th>Balance</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    @{

                                                for (var i = 0; i < Res.Count(); i++)
                                                {
                                                    if (i != 0 && Res[i].CollectorCode == Res[i - 1].CollectorCode && Res[i].DomainType == Res[i - 1].DomainType)
                                                    {
                                                        <tr>
                                                            <td></td>
                                                            <td></td>
                                                            <td></td>
                                                            <td>@Res[i].Product</td>
                                                            <td>@Res[i].CountOfCustomers</td>
                                                            <td>@Res[i].CountOfPayment</td>
                                                            <td>@Res[i].Payment</td>
                                                            <td>@Res[i].Balance</td>
                                                            <td>@Res[i].PastDue</td>
                                                        </tr>
                                                      }
                                                else if (i != 0 && Res[i].CollectorCode == Res[i - 1].CollectorCode && Res[i].DomainType != Res[i - 1].DomainType)
                                                {
                                                        <tr>
                                                            <td></td>
                                                            <td></td>
                                                            <td>@Res[i].DomainType</td>
                                                            <td>@Res[i].Product</td>
                                                            <td>@Res[i].CountOfCustomers</td>
                                                            <td>@Res[i].CountOfPayment</td>
                                                            <td>@Res[i].Payment</td>
                                                            <td>@Res[i].Balance</td>
                                                            <td>@Res[i].PastDue</td>
                                                        </tr>
                                                }
                                                else
                                                {
                                                        <tr>
                                                            <td>@Res[i].CollectorCode</td>
                                                            <td>@Res[i].CollectorName</td>
                                                            <td>@Res[i].DomainType</td>
                                                            <td>@Res[i].Product</td>
                                                            <td>@Res[i].CountOfCustomers</td>
                                                            <td>@Res[i].CountOfPayment</td>
                                                            <td>@Res[i].Payment</td>
                                                            <td>@Res[i].Balance</td>
                                                            <td>@Res[i].PastDue</td>
                                                        </tr>
                                                }
                                            }
                                                    }
                                                </tbody>
                                            </table>

                            }
                            else if (ViewBag.CollectorReportwarning != null)
                            {
                               <p class="text-warning"><span class="fa fa-warning text-yellow"></span> Collector Report : @ViewBag.CollectorReportwarning</p>
                            }
                            
}
<script src="~/Scripts/Custom/DatePicker/globalize.min.js"></script>
<script src="~/Scripts/Custom/DatePicker/moment.min.js"></script>
<script src="~/Scripts/Custom/DatePicker/1.js"></script>
<script src="~/Scripts/Custom/DatePicker/2.js"></script>
<script src="~/Scripts/Custom/DatePicker/utility.js"></script>
<script src="~/Scripts/Custom/DatePicker/demo.js"></script>
<script src="~/Scripts/Custom/DatePicker/main.js"></script>

<script>

    $('#datepicker').datetimepicker({
        format: "YYYY", // Notice the Extra space at the beginning
        viewMode: "years",
        minViewMode: "years",
        autoclose: true
    });
    
</script>