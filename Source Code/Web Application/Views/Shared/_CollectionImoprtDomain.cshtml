@{EIF.Helper.collection.Get get = new EIF.Helper.collection.Get();}

@using (Ajax.BeginForm("ImportDomain", "collection",
                                     new AjaxOptions
                                     {
                                         //AllowCache = false,
                                         HttpMethod = "POST",
                                         UpdateTargetId = "ImportMsg",
                                         InsertionMode = InsertionMode.Replace,
                                         OnBegin = "ShowProgress();"
                                         , OnComplete = "HideProgress();"
                                        // OnSuccess = "Step3"
                                        
                                     }))
{
    <div class="panel panel-default">
        <div class="panel-heading">
            Import Domain
        </div>
        <div class="panel-body">
            <div id="container">
                <div class="col-lg-12">
                    
                    <div class="row">
                        <div class="form-group">
                            <div class="row">
                                <div class="col-lg-4">
                                    <label class="control-label">Schema</label>
                                    <input type="hidden" id="DomainID2" name="DomainID2" value="0" />
                                    @Html.DropDownList("SchemaID", get.SchemaList(null, null) as SelectList, "Choose", new
                                                {
                                                    @class = "form-control",
                                                    @onchange="SelectFile()"
                                                })
                                </div>
                                <div class="col-lg-4">
                                    <br />
                                    <center>
                                        <span id="selectF" class="btn btn-success fileinput-button hide" style="width:200px">
                                            <i class="glyphicon glyphicon-plus"></i>
                                            <span>Select Domain File</span>
                                            <input type="file" name="files[]" id="file">
                                        </span>
                                    </center>
                                    <input type="hidden" name="excelFile" id="excelFile" value="" />
                                </div>
                                <div class="col-lg-4"></div>                                
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group">
                            <div class="row" id="sheets"> </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group">
                            <div class="row">
                                <div id="Statistic"></div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>


        </div>
    </div>
 
}
 

<script src="~/Scripts/Custom/PublicConfig.js"></script>
<script src="~/Scripts/Custom/Collection.js"></script>